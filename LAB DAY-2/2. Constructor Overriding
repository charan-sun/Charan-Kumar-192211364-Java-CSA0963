class Base {
    int a, b;

    Base() {
        a = 10;
        b = 15;
    }

    Base(int a, int b) {
        this.a = a;
        this.b = b;
    }

    void disp() {
        System.out.println("a = " + a + ", b = " + b);
    }
}

public class Main {
    public static void main(String[] args) {
        Base obj1 = new Base();
        obj1.disp();
        Base obj2 = new Base(20, 25);
        obj2.disp();
    }
}
